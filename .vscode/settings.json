{
  "highlight.regexes": {
    "(\"@_[A-Z_]+\": {},)": {
      "filterFileRegex": ".*\\.arb",
      "decorations": [
        {
          "backgroundColor": "green",
          "fontWeight": "bold"
        }
      ]
    }
  },

  // FVM
  "dart.flutterSdkPath": ".fvm/flutter_sdk",
  // Remove .fvm files from search
  "search.exclude": {
    "**/.fvm": true
  },
  // Remove from file watching
  "files.watcherExclude": {
    "**/.fvm": true
  }
}
